/* 这里的样式在 hyui.css里面有，因为会被bootstrap覆盖，暂时先放在这里，后期在慢慢删除 start*/
* {
  word-wrap: break-word; -webkit-box-sizing: content-box; -moz-box-sizing: content-box; box-sizing: content-box
}

html,body {
  height: 100%
}

html,body,h1,h2,h3,h4,h5,h6,hr,p,iframe,dl,dt,dd,ul,ol,li,pre,form,button,input,textarea,th,td,fieldset {
  margin: 0; padding: 0
}

body {
  font-family: arial, "宋体", "微软雅黑", "open sans", "Helvetica Neue", Helvetica, Arial, sans-serif; font-size: 12px; color: #000; overflow-x: hidden
}

input,button {
  overflow: visible; vertical-align: middle; outline: none
}

select {
  width: 100%; height: 100%; border: 1px solid #aaa; background-color: #fff; color: #555
}

option {
  font-weight: normal; display: block; white-space: pre; min-height: 1.2em; padding: 0px 2px 1px
}

dt,ul,ol {
  list-style: none
}

h1,h2,h3,h4,h5,h6 {
  font-size: 100%
}

img {
  border: none
}

td {
  color: #555
}

:focus {
  outline: none
}

b,strong {
  font-weight: normal
}
/*1-1.清除浮动*/
.cl:after,.clearfix:after {
  content: "\20"; display: block; height: 0; clear: both; visibility: hidden
}

.cl,.clearfix {
  zoom: 1
}
/* * -----2.整体布局------ */
input,label {
  font-size: 12px; color: #555
}

label {
  font-weight: normal; max-width: none;
}

input:-webkit-autofill {
  -webkit-text-fill-color: #666 !important; transition: background-color 5000s ease-in-out 0s
}

input[type=text],input[type=password] {
  text-indent: 2px
}

input[type=radio],input[type=checkbox] {
  margin: 0; cursor: pointer
}

.form_content input[readonly] {
  background-color: #f1f1f1
}

.order-info input[readonly] {
  background-color: #f6f5f5
}

textarea[readonly] {
  background-color: #f1f1f1
}

textarea {
  text-indent: 2px; overflow-x: hidden; border: 1px solid #aaa; border-radius: 3px; -moz-border-radius: 3px;
}

.wrapper {
  position: relative; margin: 0 auto; width: 100%; height: 100%; min-width: 800px; width: auto; _width: 800px; overflow: auto
}

.mar_top10 {
  margin-top: 10px
}

.boot-mar {
  margin-top: -28px;
}

.center {
  text-align: center;
}

