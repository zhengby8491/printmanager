.hy_workdesk {
	width: 100%;
	height: 100%;
	background: #fff;
	font-family: "微软雅黑";
	box-sizing: border-box;
	padding: 10px;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}

.hy_workdesk .hy_left {
	position: absolute;
	left: 10px;
	right: 265px;
	top: 10px;
	bottom: 10px;
	left: 10px;
	border-radius: 5px;
	-moz-border-radius: 5px;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}

.hy_workdesk .hy_right {
	position: relative;
	float: right;
	width: 245px;
	height: 100%;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}

.hy_workdesk .hy_left_bottom {
	position: absolute;
	top: 307px;
	bottom: 0;
	left: 0;
	width: 100%;
}

.hy_workdesk .dateClass {
	padding-left: 10px;
	line-height: 30px;
	border-bottom: 2px solid #00a477;
}

.hy_workdesk .dateClass li {
	float: left;
	font: 12px/26px "微软雅黑"
}

.hy_workdesk .dateClass li a {
	margin: 0 5px;
	color: #5e5e5e
}

.hy_workdesk .dateClass li.active a {
	color: #ffb256
}

.hy_workdesk .dataItems_wrap {
	position: relative;
	padding: 0 30px;
	overflow: hidden;
}

.hy_workdesk .dataItems {
	width: 1200px;
	margin: 0 auto
}

.hy_workdesk .data_item {
	position: relative;
	float: left;
	padding: 0 15px;
	width: 17%;
	height: 200px;
	border-radius: 10px;
	-moz-border-radius: 10px;
	text-align: center
}

.hy_workdesk .data_item.active {
	background-color: #44AFDC
}

.hy_workdesk .slide_btn {
	display: none;
	position: absolute;
	top: 90px;
	width: 24px;
	height: 24px;
	background-color: #0071CE;
	z-index: 99999;
	cursor: pointer;
}

.hy_workdesk .slide_btn_left {
	left: 15px;
	background: url(../images/menu/btn_left.png) no-repeat;
}

.hy_workdesk .slide_btn_right {
	right: 15px;
	background: url(../images/menu/btn_right.png) no-repeat;
}

.hy_workdesk .d_line {
	display: none;
	position: absolute;
	top: -2px;
	left: 50%;
	z-index: 9;
	margin-left: -30%;
	width: 60%;
	height: 5px;
	background-color: #ffb256;
}

.hy_workdesk .d_icon {
	position: absolute;
	top: 25px;
	left: 50%;
	margin-left: -40px;
	z-index: 2
}

.hy_workdesk .d_bg {
	display: none;
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	border-radius: 10px;
	-moz-border-radius: 10px;
	z-index: 1;
	height: 100%;
}

.hy_workdesk .d_content {
	position: absolute;
	padding: 5px 0 0 2px;
	z-index: 2;
	border: 1px solid #f8f8f8;
	box-shadow: 1px 1px 2px #ccc;
	overflow: hidden;
	white-space: nowrap;
	left: 10px;
	right: 10px;
	bottom: 20px;
	height: 60px;
	background: #fff;
	border-radius: 6px;
	-moz-border-radius: 6px;
}

.hy_workdesk .d_content p {
	cursor: default;
}

.hy_workdesk .d_content p span {
	line-height: 20px;
	font-size: 14px;
	color: #656565
}

.hy_workdesk .d_content p b {
	display: inline-block;
	margin-left: 6px;
	font-size: 16px;
	line-height: 24px;
	color: #ffb256
}

.hy_workdesk .hy_chart {
	margin-top: 5px;
	width: 100%
}

.hy_workdesk .chart_container {
	width: 100%
}

.hy_workdesk .r_item {
	margin-bottom: 10px;
	padding-bottom: 2px;
	border: 1px solid #a0a0a0;
	border-radius: 5px;
	-moz-border-radius: 5px;
	min-height: 20px;
	overflow: hidden
}

.hy_workdesk .r_item h4 {
	line-height: 26px;
	border-bottom: 2px solid #00a477;
	text-align: center;
}

.hy_workdesk .r_item h4 span {
	padding: 2px 12px;
	font: 14px/26px "微软雅黑";
	color: #FFB256;
	border-bottom: 4px solid #FFB256;
	letter-spacing: 2px;
}

.hy_workdesk .r_item .warn_clock {
	display: inline-block;
	vertical-align: sub;
}

.hy_workdesk .item_list {
	padding: 2px 5px 0;
}

.hy_workdesk .item_list li {
	position: relative;
	line-height: 30px;
	width: 100%;
	height: 30px;
	border-bottom: 1px dotted #ccc;
	font-size: 13px;
	letter-spacing: 1px;
	cursor: pointer
}

.hy_workdesk .item_list li.border0 {
	border-bottom: 0;
}

.hy_workdesk .item_list li:hover a {
	color: #000;
	text-decoration: none;
	text-shadow: 2px 2px 2px #ccc;
}

.hy_workdesk .item_icon {
	position: absolute;
	left: 5px;
	top: 10px;
	width: 10px;
	height: 10px;
	background: url(../images/welcome_circle.png) no-repeat;
}

.hy_workdesk .item_content {
	display: inline-block;
	margin-left: 20px;
	min-width: 40px;
	max-width: 70%;
	color: #656565;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.hy_workdesk .view {
	position: absolute;
	right: 5px;
	color: #080
}

.hy_workdesk .notice {
	padding: 5px 10px;
	height: 40px
}

.hy_workdesk .info-box {
	width: 100%;
	padding: 8px 20px;
	color: white;
	-webkit-box-shadow: inset 0 0 1px 1px rgba(255, 255, 255, 0.35), 0 3px 1px -1px rgba(0, 0, 0, 0.1);
	-moz-box-shadow: inset 0 0 1px 1px rgba(255, 255, 255, 0.35), 0 3px 1px -1px rgba(0, 0, 0, 0.1);
	box-shadow: inset 0 0 1px 1px rgba(255, 255, 255, 0.35), 0 3px 1px -1px rgba(0, 0, 0, 0.1);
	border-radius: 6px 6px 0 0;
	box-sizing: border-box;
}

.hy_workdesk .info-box.red-bg {
	background: #d95043;
}

.hy_workdesk .info-box.green-bg {
	background: #26c281;
}

.hy_workdesk .info-box.blue-bg {
	background: #57889c;
}

.hy_workdesk .todo_item {
	width: 32.66%;
